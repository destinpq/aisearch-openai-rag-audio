{
  "name": "aisearch-openai-rag-audio",
  "version": "1.0.0",
  "description": "AI Search OpenAI RAG Audio Application",
  "private": true,
  "workspaces": [
    "app/frontend"
  ],
  "scripts": {
    "build": "npm run build --workspace=app/frontend",
    "dev": "npm run dev --workspace=app/frontend",
    "start": "npm run start --workspace=app/frontend",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs aisearch-rag-backend",
    "pm2:monit": "pm2 monit"
  }
}
